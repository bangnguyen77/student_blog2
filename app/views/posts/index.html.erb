<div class="transitions-enabled" id="posts">
  <% @posts.each do |post| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag post.image.url(:medium)), post %>
      <div class="panel-body">
        <h2>
          <%= link_to post.title, post_path(post) %>
        </h2>
        <p class="user"></p>
        Submitted by
        <%= post.user.email %>
      </div>
    </div>
  <% end %>
</div>
